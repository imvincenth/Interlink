      window.xURL = "<%= image_url("x.svg") %>"
      window.submitPhotoURL = "<%= image_url("submitphoto.svg") %>"
      window.submitVideoURL = "<%= image_url("submitvideo.svg") %>"
      window.awsURL = "<%= image_url("awslogo.svg") %>"
      window.postEditURL = "<%= image_url("postedit.svg") %>"
    </script>

    <link rel="icon" type="image/x-icon" href="<%= image_url('ringin.ico') %>" />
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>